query OrgProject($login:String!$number:Int!){organization(login: $login){projectV2(number: $number)
        {number,url,shortDescription,public,closed,title,id,readme,
    items{totalCount
            },
    fields{totalCount
            },
    owner{__typename,... on User{login
                },... on Organization{login
                }
            }
        },login
    }
}